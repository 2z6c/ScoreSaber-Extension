{
  "name": "scoresaber-extension",
  "version": "1.0.1",
  "description": "Browser Extension for scoresaber.com",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile:sass": "sass src/scss/header.scss:dist/header.css src/scss/songPage.scss:dist/songPage.css src/scss/userProfile.scss:dist/userProfile.css src/scss/popup.scss:dist/popup.css src/scss/leaderboard.scss:dist/leaderboard.css",
    "compile:pug": "pug src/ui/popup.pug -o dist/",
    "build": "node build.js production & npm run compile:pug",
    "watch": "node build.js development 1"
  },
  "author": "nizhack",
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "esbuild": "^0.12.8",
    "esbuild-sass-plugin": "^1.4.6",
    "eslint": "^7.28.0",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.34.1"
  }
}
