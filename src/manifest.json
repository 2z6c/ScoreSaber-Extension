{
  "name": "Score Saber Extension",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://scoresaber.com/u/*"],
      "js": ["js/userProfile.js"],
      "css": ["userProfile.css"]
    }, {
      "matches": ["https://scoresaber.com/leaderboard/*"],
      "js": ["js/songPage.js"],
      "css": ["songPage.css"]
    }, {
      "matches": ["https://scoresaber.com/","https://scoresaber.com/?*"],
      "js": ["js/leaderboard.js"],
      "css": ["leaderboard.css"]
    }, {
      "matches": ["https://new.scoresaber.com/ranking/requests"],
      "run_at": "document_idle",
      "js": ["js/rankingRequest.js"]
    }, {
      "matches": ["https://*.scoresaber.com/*"],
      "exclude_matches": ["*://*/api*"],
      "js": ["js/header.js"],
      "css": ["header.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "/icons/16x16.png",
      "32": "/icons/32x32.png",
      "48": "/icons/48x48.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/icons/16x16.png",
    "32": "/icons/32x32.png",
    "48": "/icons/48x48.png"
  }
}